
<div class='container edit-song-pane col-lg-7 col-lg-offset-3 col-xs-12 well'>

  <h4 class='col-lg-4 col-xs-4'>Songs</h4>
  <h4 class='col-lg-4 col-xs-4'>Artist</h4>
  <h4 class='col-lg-3 col-xs-3'>Runtime</h4>

  {{#each songs as |song|}}

  <!-- {{> partial}} -->
    <div data-id="{{song.id}}" class='row edit-song'>
      <div class='col-lg-4 col-xs-4'>
        {{song.name}}
      </div>
      <div class='col-lg-4 col-xs-4'>
          {{song.artist}}
      </div>
      <div class='col-lg-3 col-xs-3'>
          {{song.runtime}}
      </div>
    </div>
  {{/each}}
</div>
