<div class='row headerz col-lg-offset-1'>
  <h3 class='col-lg-4 col-xs-4'>Songs</h3>
  <h3 class='col-lg-4 col-xs-4'>Artist</h3>
  <h3 class='col-lg-3 col-xs-3'>Runtime</h3>
</div>

{{#each user_songs as |user_song|}}

  <!-- {{> partial}} -->
    <div data-id="{{user_song.id}}" class='row col-lg-offset-1'>
      <div class='col-lg-4 col-xs-4'>
        {{user_song.song.name}}
      </div>
      <div class='col-lg-4 col-xs-4'>
          {{user_song.song.artist}}
      </div>
      <div class='col-lg-3 col-xs-3'>
          {{user_song.song.runtime}}
      </div>
    </div>
{{/each}}
