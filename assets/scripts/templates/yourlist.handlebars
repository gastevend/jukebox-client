{{#each user_songs as |user_song|}}

  <!-- {{> partial}} -->
    <div data-id="{{user_song.id}}" class='row one-song'>
      <div class='col-lg-4 text-center'>
        {{user_song.song.name}}
      </div>
      <div class='col-lg-4 text-center'>
          {{user_song.song.artist}}
      </div>
      <div class='col-lg-3 text-center'>
          {{user_song.song.runtime}}
      </div>
      <div class='col-lg-1'>
        <button type="button" data-id="{{user_song.id}}" class="btn btn-default edit">Edit</button>
      </div>
    </div>
{{/each}}
